          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion - DateTime within 5 mins" enabled="true">
            <stringProp name="BeanShellAssertion.query">import java.util.regex.Pattern;  
import java.util.regex.Matcher;
import java.util.Locale;
import java.time.*;
import java.time.format.DateTimeFormatter;
import java.time.temporal.ChronoUnit;

DateTimeFormatter formatter = DateTimeFormatter.ofPattern(&quot;EEE, d MMM yyyy HH:mm:ss z&quot;, Locale.ENGLISH);

String respData = new String(ResponseData);
String respHdrData = new String(ResponseHeaders);

//	String respHdrData = new String(&quot;Date: Tue, 29 Nov 2022 21:54:01 GMT&quot;); 
Pattern pattern = Pattern.compile(&quot;Date:\\s+(.+)&quot;);    
Matcher matcher = pattern.matcher(respHdrData);
String respDate = &quot;&quot;;
       
if (matcher.find()) {
	System.out.println(&quot;matched: &quot;+matcher.group(1));
	respDate = matcher.group(1);
}
// Get the local time in UTC!! (Without this time will be local time and the gap will be miscalculated)
LocalDateTime now = LocalDateTime.now(ZoneOffset.UTC);
LocalDateTime dt = now;
System.out.println(&quot;time now is: &quot;+dt);

try {
// parse the input
	dt = LocalDateTime.parse(respDate, formatter);
	// Date dt = ft.parse(respDate);
	System.out.println(&quot;Response Date: &quot;+dt);
	long gap = Math.abs(ChronoUnit.MINUTES.between(dt, now));
	boolean lessthan5 = gap&lt;5;
	System.out.println(&quot;gap=&quot;+gap);
	System.out.println(&quot;gap_lessthan_5=&quot;+lessthan5.toString().toUpperCase());
} catch (Exception e) { 
	System.out.println(&quot;Unparseable using &quot; + formatter); 
}
// System.out.println(&quot;earlier: &quot;+dt.minus(Duration.parse(&quot;PT10M&quot;)));
</stringProp>
